<div class="form-group">
  <label for="sel1">Get employee time registrations </label>
  <select class="form-control" id="sel1" ng-model="reqEmployee" ng-options="name for name in names">{{name}}</select>
  <button type="button" class="btn btn-primary" ng-click="getEmployeeData()">Go</button>
</div>

<!--<input class="form-control" ng-model="query" placeholder="Search for employees" autofucus> -->
<table class="table table-hover" ng-show="data">
  <thead>
    <tr>
      <th>Name</th>
      <th>Date</th>
      <th>Time</th>
      <th>Action</th>
    </tr>
  </thead>
  <tbody>
    <tr ng-repeat="record in data">
      <td> {{record.Name | uppercase}} </td>
      <td> {{record.Date}} </td>
      <td> {{record.Time}} </td>
      <td> {{record.Action | uppercase}} </td>
    </tr>
  </tbody>
</table>



<table class="table table-hover" ng-show="data">
  <thead>
    <tr>
      <th>Date</th>
      <th>Start Work</th>
      <th>Leave Work</th>
      <th>Total Work</th>
    </tr>
  </thead>
  <tbody>
    <tr ng-repeat="record in dataProcessed">
      <td> {{record.Dpoate}} </td>
      <td> {{record.startWork}} </td>
      <td> {{record.endWork}} </td>
      <td> {{record.totalWork}} </td>
    </tr>
  </tbody>
</table>